# Check Vero ICP dApp Environment Variables

# Canister IDs (populated after deployment)
CANISTER_ID_check_vero_backend=
CANISTER_ID_check_vero_frontend=

# Network Configuration
DFX_NETWORK=local

# Internet Computer Settings
# For local development
IC_LOCAL_HOST=http://localhost:4943

# For IC mainnet deployment
IC_MAINNET_HOST=https://ic0.app

# Development Settings
NODE_ENV=development
REACT_APP_IC_HOST=${IC_LOCAL_HOST}
REACT_APP_IC_WHITELIST=${CANISTER_ID_check_vero_backend}

# Features Flags
ENABLE_MOCK_DATA=true
ENABLE_DEBUG_LOGS=true

# Sample Data Configuration
INITIALIZE_SAMPLE_DATA=true